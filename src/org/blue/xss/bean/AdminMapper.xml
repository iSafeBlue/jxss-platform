<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="admin">
	<select id="checkAdminLogin" resultType="Admin" parameterType="Admin">
		select * from xss_admin where adminname = #{adminname} and adminpass = #{adminpass};
	</select>
	
	
</mapper>